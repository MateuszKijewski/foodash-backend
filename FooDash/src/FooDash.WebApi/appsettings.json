{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "AllowedOrigins": "*",
    "ConnectionStrings": {
        "FooDashDatabase": "ENTER_CONNECTION_STRING"
    },
